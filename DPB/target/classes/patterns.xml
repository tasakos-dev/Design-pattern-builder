<GoF>
	<category id="creational">
		<pattern id="Abstract factory">
			<class id="AbstractFactory" isAbstract="true">
					<method id="create" isAbstract="true">
						<type>Object</type>
					</method>
			</class>
			<class id="concreteFactoryA" extends="AbstractFactory">
			</class>
			<properties>
				<preferedClassName>concrete</preferedClassName>
			</properties>
		</pattern>
		<pattern id="Factory Method">
		<interface id="FactoryMethod">
					<method id="create">
						<type>Object</type>
					</method>
			</interface>
			<class id="concreteFactoryA" implements="FactoryMethod">
			</class>
			<properties>
				<preferedClassName>concrete</preferedClassName>
			</properties>
		</pattern>
		<pattern id="Parameterized factory">
			<class id="Factory">
					<method id="create">
						<type>Object</type>
					</method>
			</class>	
		</pattern>
		<pattern id="Singleton">
			<class id="Singleton">
				<field id="instance" isStatic="true">
					<type>Singleton</type>
				</field>
				<method id="getInstance" isStatic="true">
					<type>Object</type>
					<code>
						if (instance == null) {
							instance = new Singleton();
						}
						return instance;
					</code>
				</method>
			</class>
		</pattern>
	</category>
	<category id="creational1">
		<pattern id="parametirized1 factory">
		</pattern>
	</category>
</GoF>